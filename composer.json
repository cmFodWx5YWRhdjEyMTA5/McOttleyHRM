{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "illuminate/html": "^5.0",
        "fzaninotto/faker": "^1.5",
        "barryvdh/laravel-ide-helper": "^2.1",
        "regulus/activity-log": "0.5.*",
        "maddhatter/laravel-fullcalendar": "^1.2",
        "uxweb/sweet-alert": "~1.1",
        "zizaco/entrust": "dev-laravel-5",
        "owen-it/laravel-auditing": "^2.2",
        "maatwebsite/excel": "~2.1.0",
        "intervention/image": "^2.3",
        "guzzlehttp/guzzle": "~5.0",
        "cossou/jasperphp": "^2.1",
        "jenky/laravel-plupload": "^1.2",
        "sahusoftcom/eloquent-image-mutator": "dev-master",
        "creativeorange/gravatar": "~1.0",
        "laravolt/avatar": "~0.3",
        "codingo-me/dropzoner": "^1.0",
        "spatie/laravel-newsletter": "^3.0",
        "simplesoftwareio/simple-qrcode": "~1",
        "barryvdh/laravel-dompdf": "0.6.*",
        "vinkla/hashids": "^2.3",
        "milon/barcode": "^5.2",
        "captcha-com/laravel-captcha": "^4.1",
        "maknz/slack": "^1.7",
        "spatie/laravel-backup": "^3.9",
        "hashids/hashids": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "OrionMedical\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
